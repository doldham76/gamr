<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="nerdytinder.SetAliasPage">
  <ContentPage.Content>
    <StackLayout>
      <Image x:Name="labelUrl" HorizontalOptions="Center"/>
      <Label x:Name="labelName" TextColor="Black" HorizontalOptions="Center" />
      <Label x:Name="labelEmail" TextColor="Black" HorizontalOptions="Center" />
      <Picker x:Name="state" Title="State" HorizontalOptions="Center" WidthRequest="150" />
      <Entry x:Name="zip" Placeholder="Zip Code" HorizontalOptions="Center" WidthRequest="150" Keyboard="Numeric" />
      <Button x:Name="save" Text="Save" HorizontalOptions="Center" WidthRequest="100" />
    </StackLayout>
  </ContentPage.Content>
</ContentPage>